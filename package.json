{
  "name": "vue-tag-autocomplete",
  "version": "0.0.6",
  "description": "",
  "main": "dist/vue-tag-input.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "poi",
    "build": "poi build --format umd && poi build --component",
    "test": "poi test && jest",
    "test:update": "poi test && jest -u",
    "lint": "eslint . --ext .vue --ext .js",
    "lint:fix": "eslint --fix . --ext .vue --ext .js",
    "clean": "rm -rf dist"
  },
  "author": "Lucien Lee <lucien@lucienlee.cc>",
  "contributors": [
    {
      "name": "Julien Croain"
    }
  ],
  "repository": "git@github.com:JulienCroain/vue-tag-input.git",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.0.0-beta.2",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "keycode": "^2.1.9",
    "node-sass": "^4.7.2",
    "poi": "^9.6.4",
    "poi-preset-eslint": "^9.0.1",
    "poi-preset-transform-test-files": "^10.0.0",
    "postcss-import": "^11.1.0",
    "postcss-url": "^7.3.1",
    "sass-loader": "^6.0.6",
    "vue-test-utils": "^1.0.0-beta.9"
  },
  "peerDependencies": {
    "vue": "^2.5.13"
  },
  "dependencies": {
    "string-width": "^2.1.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.transformed.jsx?$",
    "mapCoverage": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
